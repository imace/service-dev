<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sonymobile.sonysales.model.Administrator" table="Administrator">

		<id name="id" column="id" type="long">
			<generator class="increment" />	<!-- 设置ID字段自动增值 -->
		</id>
    <property name="userName" type="java.lang.String">
        <column name="userName" length="20" not-null="true">
            <comment>用户名</comment>
        </column>
    </property>
    <property name="password" type="java.lang.String">
        <column name="password" length="20" not-null="true">
            <comment>密码</comment>
        </column>
    </property>
		<property name="permissionId" type="long">
			<column name="permissionId" length="20" not-null="false">
				<comment>权限id</comment>
			</column>
		</property>
    <property name="state" type="int">
        <column name="state" length="20" not-null="true">
            <comment>状态</comment>
        </column>
    </property>
		<property name="createTime" type="java.lang.String">
			<column name="createTime" length="12" not-null="true">
				<comment>创建时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
